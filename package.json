{
  "name": "@amvasdev/react-daisyui",
  "sideEffects": false,
  "type": "module",
  "version": "0.0.3",
  "description": "Customizable react components that include daisyui and tailwind classes",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "build": "microbundle --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react && cp package.json dist/package.json && cp README.md dist/README.md",
    "dev": "microbundle watch --jsx react -f es,cjs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Amaury-Vasquez/-amvasdev-react-daisyui.git"
  },
  "keywords": [
    "React",
    "DaisyUI",
    "Tailwindcss",
    "Components"
  ],
  "author": "@amvasdev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Amaury-Vasquez/-amvasdev-react-daisyui/issues"
  },
  "homepage": "https://github.com/Amaury-Vasquez/-amvasdev-react-daisyui#readme",
  "devDependencies": {
    "@types/react": "^18.2.15",
    "daisyui": "^3.2.1",
    "microbundle": "^0.15.1",
    "np": "^8.0.4",
    "react": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.8.0",
    "npm": ">=7.5.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "np": {
    "contents": "dist"
  }
}
